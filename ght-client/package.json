{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,svelte}'",
    "build:production": "NODE_ENV=production rollup -c",
    "build:development": "NODE_ENV=development rollup -c",
    "dev": "NODE_ENV=local rollup -c -w",
    "start": "sirv public --no-clear -p 5025 --single",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/node": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "autoprefixer": "^10.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.3.6",
    "postcss-load-config": "^3.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.9.4",
    "tailwindcss": "^2.2.14",
    "tslib": "^2.0.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@dopry/svelte-auth0": "^2.0.0",
    "@microsoft/signalr": "^5.0.9",
    "history": "^5.0.0",
    "sirv-cli": "^1.0.0",
    "svelte-navigator": "^3.1.5"
  }
}
