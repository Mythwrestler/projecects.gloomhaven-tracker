{
    "image": "registry.casperinc.net/vscode/devcontainers/dotnet:6.0",
    "remoteUser": "vscode",
    "forwardPorts": [5020],
    "extensions": [
        "streetsidesoftware.code-spell-checker",
        "eamodio.gitlens",
        "codezombiech.gitignore",
        "heaths.vscode-guid",
        "ms-dotnettools.csharp",
        "patcx.vscode-nuget-gallery",
        "leo-labs.dotnet",
    ],
    "settings": {
        "editor.semanticHighlighting.enabled": true,
        "csharp.semanticHighlighting.enabled": true,
        "cSpell.words": [
            "Npgsql",
            "Unprocessable"
        ],
        "cSpell.ignorePaths": [
            "package-lock.json",
            "node_modules",
            "vscode-extension",
            ".git/objects",
            ".vscode",
            ".vscode-insiders",
            ".devcontainer"
        ]
    }
}