    "version": "0.2.0",
    "configurations": [
        
      {
          "name": ".NET Core Launch (web)",
          "type": "coreclr",
          "request": "launch",
          "preLaunchTask": "build",
          "program": "${workspaceFolder}/bin/Debug/net6.0/ght-service.dll",
          "args": [],
          "cwd": "${workspaceFolder}",
          "stopAtEntry": false,
          "env": {
              "ASPNETCORE_URLS": "http://localhost:5020",
              "ASPNETCORE_ENVIRONMENT": "Development",
              "AUTH_ENABLED": "false",
              "AUTH_AUTHORITY": "http://fake-authority/",
              "AUTH_AUDIENCE": "fake-audience",
              "HTTP_LOGGING_ENABLED": "false"
          },
          "sourceFileMap": {
              "/Views": "${workspaceFolder}/Views"
          },
          "justMyCode": true
      }
    ]
}